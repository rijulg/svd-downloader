{
    "name": "svd-downloader",
    "dockerComposeFile": "docker-compose.yml",
    "service": "dev",
    "workspaceFolder": "/home/workspace",
    "customizations": {
      "vscode": {
        "extensions": [
          "eamodio.gitlens",
          "esbenp.prettier-vscode",
          "ms-python.black-formatter",
          "ms-python.flake8",
          "ms-python.python"
        ]
      }
    },
    "onCreateCommand": "cd /home/workspace && pip install pipenv && pipenv --site-packages install"
  }